{
  "name": "lab_6_unit_tests",
  "version": "1.0.0",
  "description": "Simple JS app with unit tests",
  "main": "app.js",
  "jest-junit": {
    "outputDirectory": "./coverage"
  },
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage --reporters=\"jest-junit\""
  },
  "author": "Siarhei Kharlanau <s.v.harlanov@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "jest": "^27.3.1",
    "jest-junit": "^13.0.0",
    "nyc": "^15.1.0"
  },
  "nyc": {
    "all": true,
    "include": [
      "*.js"
    ],
    "exclude": [
      "**/*.spec.js"
    ],
    "reporter": [
      "html",
      "cobertura"
    ]
  }
}
